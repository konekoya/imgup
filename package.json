{
  "name": "imgup",
  "version": "1.2.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"**/**/*.{ts,js,tsx,jsx,md,html,css}\"",
    "format:check": "prettier --check \"**/**/*.{ts,js,tsx,jsx,md,html,css}\"",
    "build": "tsc",
    "start": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run format && npm run typecheck"
    }
  },
  "bin": {
    "imgup": "./dist/index.js"
  },
  "dependencies": {
    "axios": "^1.2.6",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "ora": "^6.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/form-data": "^2.5.0",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.18.11",
    "typescript": "^4.9.5",
    "prettier": "^2.4.1",
    "husky": "^4.3.8"
  }
}
